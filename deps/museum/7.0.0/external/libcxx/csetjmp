#ifndef _MUSEUM_LIBCPP_CSETJMP
#define _MUSEUM_LIBCPP_CSETJMP

/*
    csetjmp synopsis

Macros:

    setjmp

namespace std
{

Types:

    jmp_buf

void longjmp(jmp_buf env, int val);

}  // std

*/

#include <museum/7.0.0/external/libcxx/__config>
#include <museum/7.0.0/bionic/libc/setjmp.h>

#if !defined(_MUSEUM_LIBCPP_HAS_NO_PRAGMA_SYSTEM_HEADER)
#pragma GCC system_header
#endif

#ifndef setjmp
#define setjmp(env) setjmp(env)
#endif

_MUSEUM_LIBCPP_BEGIN_NAMESPACE_STD

using ::jmp_buf;
using ::longjmp;

_MUSEUM_LIBCPP_END_NAMESPACE_STD

#endif  // _MUSEUM_LIBCPP_CSETJMP
