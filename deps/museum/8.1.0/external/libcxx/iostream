#ifndef _MUSEUM_LIBCPP_IOSTREAM
#define _MUSEUM_LIBCPP_IOSTREAM

/*
    iostream synopsis

#include <museum/8.1.0/external/libcxx/ios>
#include <museum/8.1.0/external/libcxx/streambuf>
#include <museum/8.1.0/external/libcxx/istream>
#include <museum/8.1.0/external/libcxx/ostream>

namespace std {

extern istream cin;
extern ostream cout;
extern ostream cerr;
extern ostream clog;
extern wistream wcin;
extern wostream wcout;
extern wostream wcerr;
extern wostream wclog;

}  // std

*/

#include <museum/8.1.0/external/libcxx/__config>
#include <museum/8.1.0/external/libcxx/ios>
#include <museum/8.1.0/external/libcxx/streambuf>
#include <museum/8.1.0/external/libcxx/istream>
#include <museum/8.1.0/external/libcxx/ostream>

#if !defined(_MUSEUM_LIBCPP_HAS_NO_PRAGMA_SYSTEM_HEADER)
#pragma GCC system_header
#endif

_MUSEUM_LIBCPP_BEGIN_NAMESPACE_STD

#ifndef _MUSEUM_LIBCPP_HAS_NO_STDIN
extern _MUSEUM_LIBCPP_FUNC_VIS istream cin;
extern _MUSEUM_LIBCPP_FUNC_VIS wistream wcin;
#endif
#ifndef _MUSEUM_LIBCPP_HAS_NO_STDOUT
extern _MUSEUM_LIBCPP_FUNC_VIS ostream cout;
extern _MUSEUM_LIBCPP_FUNC_VIS wostream wcout;
#endif
extern _MUSEUM_LIBCPP_FUNC_VIS ostream cerr;
extern _MUSEUM_LIBCPP_FUNC_VIS wostream wcerr;
extern _MUSEUM_LIBCPP_FUNC_VIS ostream clog;
extern _MUSEUM_LIBCPP_FUNC_VIS wostream wclog;

_MUSEUM_LIBCPP_END_NAMESPACE_STD

#endif  // _MUSEUM_LIBCPP_IOSTREAM
