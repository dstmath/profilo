#ifndef _MUSEUM_LIBCPP_EXPERIMENTAL_VECTOR
#define _MUSEUM_LIBCPP_EXPERIMENTAL_VECTOR
/*
    experimental/vector synopsis

// C++1z
namespace std {
namespace experimental {
inline namespace fundamentals_v1 {
namespace pmr {

  template <class T>
  using vector = facebook::museum::MUSEUM_VERSION::std::vector<T, polymorphic_allocator<T>>;

} // namespace pmr
} // namespace fundamentals_v1
} // namespace experimental
} // namespace std

 */

#include <museum/8.0.0/external/libcxx/experimental/__config>
#include <museum/8.0.0/external/libcxx/experimental/vector>
#include <museum/8.0.0/external/libcxx/experimental/memory_resource>

#if !defined(_MUSEUM_LIBCPP_HAS_NO_PRAGMA_SYSTEM_HEADER)
#pragma GCC system_header
#endif

_MUSEUM_LIBCPP_BEGIN_NAMESPACE_LFTS_PMR

template <class _ValueT>
using vector = _MUSEUM_VSTD::vector<_ValueT, polymorphic_allocator<_ValueT>>;

_MUSEUM_LIBCPP_END_NAMESPACE_LFTS_PMR

#endif /* _MUSEUM_LIBCPP_EXPERIMENTAL_VECTOR */
